<configuration>
<appender name="consoleAppender" class="ch.qos.logback.core.ConsoleAppender">
    <encoder>
        <pattern>%date{yyyy-MM-dd HH:mm:ss.SSS} [%thread] %-5level - %msg%n</pattern>
    </encoder>
</appender>
    <root level="INFO">
        <appender-ref ref="consoleAppender" />
    </root>
</configuration>


